page{
  background-color: #F7F8F9;
}
.fl{
  float: left;
}
.fr{
  float: right;
}
.clearfix::after{
  content:"";
  display:block;
  clear:both;
  line-height: 0;
  opacity: 0;
  overflow: hidden;
}
.auto-img{
  width:100%;
  height: 100%;
  display: block;
}
.one-text{
  white-space:nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.two-text{
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  overflow: hidden;
}